{
	"name": "lipe",
	"version": "0.3.3",
	"description": "A Logging library utilizing Pipes",
	"typings": "lib/Types",
	"packageManager": "pnpm@6.32.2",
	"homepage": "https://lipejs.com",
	"sideEffects": false,
	"exports": {
		".": {
			"types": "./lib/Types/index.d.ts",
			"require": "./lib/CommonJS/index.js",
			"import": "./lib/ES6/index.js"
		},
		"./defaults": {
			"types": "./lib/Types/defaults.d.ts",
			"require": "./lib/CommonJS/defaults.js",
			"import": "./lib/ES6/defaults.js"
		}
	},
	"scripts": {
		"test": "jest",
		"test:script": "node ./tests/test.js",
		"clean": "rimraf lib coverage",
		"build": "node ./util/CompileProject.mjs",
		"postinstall": "node ./util/PostInstall.mjs",
		"prepublish": "pnpm run build"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/CEbbinghaus/lipe"
	},
	"keywords": [
		"logging",
		"logger",
		"pipe",
		"lightweight",
		"expandable"
	],
	"author": "CEbbinghaus <contact@cebbinghaus.com>",
	"license": "ISC",
	"devDependencies": {
		"@types/jest": "^27.4.1",
		"@types/node": "^17.0.36",
		"@typescript-eslint/eslint-plugin": "^4.33.0",
		"@typescript-eslint/parser": "^4.33.0",
		"eslint": "^7.32.0",
		"jest": "^28.1.0",
		"jest-ts-webcompat-resolver": "^1.0.0",
		"mockdate": "^3.0.5",
		"rimraf": "^3.0.2",
		"source-map": "^0.8.0-beta.0",
		"ts-jest": "^28.0.0",
		"typescript": "4.5.2"
	},
	"optionalDependencies": {
		"chalk": "^4.1.2"
	},
	"dependencies": {
		"dayjs": "^1.11.7"
	}
}
